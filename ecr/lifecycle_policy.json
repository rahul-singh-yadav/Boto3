{
    "rules": [
        {
            "rulePriority": 1,
            "description": "Expire all untagged images pushed if they are older then 1 day.",
            "selection": {
                "tagStatus": "untagged",
                "countType": "sinceImagePushed",
                "countUnit": "days",
                "countNumber": 1
            },
            "action": {
                "type": "expire"
            }
        },
        {
            "rulePriority": 2,
            "description": "Keep only one image with tags: 'test', 'latest'",
            "selection": {
                "tagStatus": "tagged",
                "tagPrefixList": ["test", "latest"],
                "countType": "imageCountMoreThan",
                "countNumber": 1
            },
            "action": {
                "type": "expire"
            }
        }

    ]
}